
  /// <summary>
        ///		Required method for Designer support - do not modify
        ///		the contents of this method with the code editor.
        /// </summary>

 private void InitializeComponent()
        {
            this.dgMappingGroups.ItemCreated += new System.Web.UI.WebControls.DataGridItemEventHandler(this.dgMappingGroups_ItemCreated);
            this.dgMappingGroups.ItemDataBound += new System.Web.UI.WebControls.DataGridItemEventHandler(this.dgMappingGroups_ItemDataBound);
        
        }
        
        /*******************Using Datagrid***************************************
            1.   //Filling Datagrid from session Data
         dsMappingData = (DataSet)Session["dsMappingProfileData"];
         
           2.      //Creating one datatable
          DataTable dtMappingGroups = new DataTable();
          
            3.      //retrieving data in datatable
          dtMappingGroups = dsMappingData.Tables[2];
          
          4.      //Bind the grid with datable
          if (dtMappingGroups != null)
            {

                dgMappingGroups.DataSource = dtMappingGroups;
                dgMappingGroups.DataBind();
            }
